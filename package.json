    "js-file-download": "^0.4.12",
    "js-yaml": "^4.1.0",
    "json-beautify": "^1.1.1",
    "monaco-editor-core": "^0.26.0",
    "monaco-languageclient": "^0.13.1-next.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.5",
    "react-resize-detector": "^6.7.4",
    "react-split-pane": "^0.1.92",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "swagger-ui-react": "^4.0.0-rc.3",
    "typescript": "^4.1.3",
    "url": "^0.11.0",
    "vscode-json-languageservice": "^4.0.2",
    "vscode-languageserver-textdocument": "^1.0.1"
