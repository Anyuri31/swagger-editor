{
  "name": "@swagger-api/swagger-ide",
  "version": "0.2.1",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "unpkg": "./dist/umd/swagger-ide.js",
  "module": "./dist/esm/swagger-ide.js",
  "browser": "./dist/esm/swagger-ide.js",
  "jsnext:main": "./dist/esm/swagger-ide.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "browser": "./dist/esm/swagger-ide.js"
    }
  },
  "scripts": {
    "prepublishOnly": "npm run build:bundle:esm && npm run build:bundle:umd",
    "start": "cross-env ENABLE_PROGRESS_PLUGIN=true react-scripts start",
    "build": "npm run build:app && npm run build:bundle:esm && npm run build:bundle:umd",
    "build:app": "cross-env ENABLE_PROGRESS_PLUGIN=false DISABLE_ESLINT_PLUGIN=false react-scripts build",
    "build:app:serve": "serve -s build -l 3050",
    "build:bundle:esm": "rimraf ./dist/esm && cross-env ENABLE_PROGRESS_PLUGIN=false GENERATE_SOURCEMAP=true BUILD_ESM_BUNDLE=true DIST_PATH=dist/esm react-scripts build-bundle",
    "build:bundle:umd": "rimraf ./dist/umd && cross-env ENABLE_PROGRESS_PLUGIN=false GENERATE_SOURCEMAP=false BUILD_UMD_BUNDLE=true DIST_PATH=dist/umd react-scripts build-bundle",
    "analyze": "source-map-explorer 'dist/esm/*.mjs'",
    "test": "react-scripts test",
    "cy:open": "cypress open",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:dev": "start-server-and-test start http://localhost:3000 cy:open",
    "cy:ci": "start-server-and-test build:app:serve http://localhost:3050 cy:run:chrome",
    "lint": "eslint . --ext .jsx,.js",
    "lint:fix": "eslint . --ext .jsx,.js --fix",
    "clean": "rimraf ./build ./dist",
    "link:apidom": "npm link @swagger-api/apidom-ast @swagger-api/apidom-core @swagger-api/apidom-parser-adapter-json @swagger-api/apidom-ns-asyncapi-2 @swagger-api/apidom-parser-adapter-yaml-1-2 @swagger-api/apidom-parser-adapter-asyncapi-yaml-2 @swagger-api/apidom-parser-adapter-openapi-yaml-3-1 @swagger-api/apidom-parser @swagger-api/apidom-parser-adapter-asyncapi-json-2 @swagger-api/apidom-ls @swagger-api/apidom-ns-openapi-3-1 @swagger-api/apidom-reference @swagger-api/apidom-parser-adapter-openapi-json-3-1",
    "link:apidom-ls": "npm link @swagger-api/apidom-ls"
  },
  "dependencies": {
    "@asyncapi/parser": "^1.13.2",
    "@asyncapi/react-component": "^1.0.0-next.28",
    "@babel/runtime": "^7.16.7",
    "@primer/octicons-react": "^16.2.0",
    "@swagger-api/apidom-ls": "^0.16.0",
    "axios": "^0.24.0",
    "deepmerge": "^4.2.2",
    "file-dialog": "^0.0.8",
    "is-json": "^2.0.1",
    "js-file-download": "^0.4.12",
    "js-yaml": "^4.1.0",
    "json-beautify": "^1.1.1",
    "monaco-editor-core": "^0.31.1",
    "monaco-languageclient": "^0.13.1-next.9",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-collapse-pane": "^2.0.1",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-resize-detector": "^7.0.0",
    "reselect": "^4.1.5",
    "swagger-ui-react": "^4.1.3",
    "vscode-languageserver-textdocument": "^1.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "react-scripts": "file:./create-react-app/packages/react-scripts",
    "rimraf": "^3.0.2",
    "sass": "^1.47.0",
    "serve": "^13.0.2",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.14.0",
    "stream-browserify": "^3.0.0",
    "web-vitals": "^2.1.3"
  }
}
