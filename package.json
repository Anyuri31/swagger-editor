    "axios": "^0.24.0",
    "deepmerge": "^4.2.2",
    "file-dialog": "0.0.8",
    "is-json": "^2.0.1",
    "js-file-download": "^0.4.12",
    "js-yaml": "^4.1.0",
    "json-beautify": "^1.1.1",
    "monaco-editor-core": "^0.29.2",
    "monaco-languageclient": "^0.13.1-next.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.2",
