{
  "name": "swagger-ide",
  "description": "Swagger IDE plugin for SwaggerUI",
  "version": "0.2.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f ./.env.development node scripts/start.js",
    "start-ci": "env-cmd -f ./.env.test node scripts/start.js",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 node scripts/build.js",
    "test": "node scripts/test.js --config config/jest/jest.config.js",
    "test:cy:open": "env-cmd -f ./.env.development cypress open",
    "test:cy:run": "env-cmd -f ./.env.development cypress run",
    "test:cy:ci": "REACT_APP_CYPRESS_MODE=headless env-cmd -f ./.env.test node scripts/test-cypress.js",
    "test:cy:dev": "REACT_APP_CYPRESS_MODE=interactive env-cmd -f ./.env.test node scripts/test-cypress.js",
    "lint": "eslint ./ --ext .jsx",
    "lint:fix": "eslint ./ --ext .jsx --fix",
    "clean": "rimraf ./build",
    "serve-static-build": "serve -s build -l 3050",
    "link-local-apidom": "npm link @swagger-api/apidom-ast @swagger-api/apidom-core @swagger-api/apidom-parser-adapter-json @swagger-api/apidom-ns-asyncapi-2 @swagger-api/apidom-parser-adapter-yaml-1-2 @swagger-api/apidom-parser-adapter-asyncapi-yaml-2 @swagger-api/apidom-parser-adapter-openapi-yaml-3-1 @swagger-api/apidom-parser @swagger-api/apidom-parser-adapter-asyncapi-json-2 @swagger-api/apidom-ls @swagger-api/apidom-ns-openapi-3-1 @swagger-api/apidom-reference @swagger-api/apidom-parser-adapter-openapi-json-3-1",
    "unlink-local-apidom": "npm unlink --global @swagger-api/apidom-ast @swagger-api/apidom-core @swagger-api/apidom-parser-adapter-json @swagger-api/apidom-ns-asyncapi-2 @swagger-api/apidom-parser-adapter-yaml-1-2 @swagger-api/apidom-parser-adapter-asyncapi-yaml-2 @swagger-api/apidom-parser-adapter-openapi-yaml-3-1 @swagger-api/apidom-parser @swagger-api/apidom-parser-adapter-asyncapi-json-2 @swagger-api/apidom-ls @swagger-api/apidom-ns-openapi-3-1 @swagger-api/apidom-reference @swagger-api/apidom-parser-adapter-openapi-json-3-1",
    "link-local-apidom-ls": "npm link @swagger-api/apidom-ls",
    "unlink-local-apidom-ls": "npm unlink --global @swagger-api/apidom-ls"
  },
  "dependencies": {
    "@asyncapi/react-component": "^1.0.0-next.21",
